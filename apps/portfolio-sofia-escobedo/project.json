{
  "name": "portfolio-sofia-escobedo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "i18n": {
    "sourceLocale": "en-US",
    "locales": {
      "es-MX": "src/locale/messages.es-MX.json"
    }
  },
  "projectType": "application",
  "generators": {
    "@schematics/angular:component": {
      "style": "sass"
    },
    "@schematics/angular:application": {
      "strict": true
    }
  },
  "sourceRoot": "apps/portfolio-sofia-escobedo/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/portfolio-sofia-escobedo",
        "index": "apps/portfolio-sofia-escobedo/src/index.html",
        "main": "apps/portfolio-sofia-escobedo/src/main.ts",
        "polyfills": "apps/portfolio-sofia-escobedo/src/polyfills.ts",
        "tsConfig": "apps/portfolio-sofia-escobedo/tsconfig.app.json",
        "aot": true,
        "assets": [
          "apps/portfolio-sofia-escobedo/src/favicon.ico",
          "apps/portfolio-sofia-escobedo/src/assets"
        ],
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
          "apps/portfolio-sofia-escobedo/src/styles.sass",
          "node_modules/prismjs/themes/prism-okaidia.css",
          "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.css",
          "node_modules/prismjs/plugins/line-highlight/prism-line-highlight.css",
          "node_modules/prismjs/plugins/command-line/prism-command-line.css"
        ],
        "scripts": [
          "apps/portfolio-sofia-escobedo/src/assets/js/roma-script.js",
          "node_modules/mermaid/dist/mermaid.min.js",
          "node_modules/marked/marked.min.js",
          "node_modules/prismjs/prism.js",
          "node_modules/prismjs/components/prism-csharp.min.js",
          "node_modules/prismjs/components/prism-css.min.js",
          "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.js",
          "node_modules/prismjs/plugins/line-highlight/prism-line-highlight.js",
          "node_modules/prismjs/plugins/command-line/prism-command-line.js",
          "node_modules/emoji-toolkit/lib/js/joypixels.min.js"
        ]
      },
      "configurations": {
        "production": {
          "localize": true,
          "fileReplacements": [
            {
              "replace": "apps/portfolio-sofia-escobedo/src/environments/environment.ts",
              "with": "apps/portfolio-sofia-escobedo/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ]
        },
        "i18n-es": {
          "localize": ["es-MX"]
        }
      }
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "browserTarget": "portfolio-sofia-escobedo:build"
      },
      "configurations": {
        "production": {
          "browserTarget": "portfolio-sofia-escobedo:build:production"
        },
        "i18n-es": {
          "browserTarget": "portfolio-sofia-escobedo:build:i18n-es"
        }
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "portfolio-sofia-escobedo:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "apps/portfolio-sofia-escobedo/src/test.ts",
        "polyfills": "apps/portfolio-sofia-escobedo/src/polyfills.ts",
        "tsConfig": "apps/portfolio-sofia-escobedo/tsconfig.spec.json",
        "karmaConfig": "apps/portfolio-sofia-escobedo/karma.conf.js",
        "assets": [
          "apps/portfolio-sofia-escobedo/src/favicon.ico",
          "apps/portfolio-sofia-escobedo/src/assets"
        ],
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
          "apps/portfolio-sofia-escobedo/src/styles.sass"
        ],
        "scripts": []
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/portfolio-sofia-escobedo/**/*.ts",
          "apps/portfolio-sofia-escobedo/**/*.html"
        ]
      }
    }
  }
}
